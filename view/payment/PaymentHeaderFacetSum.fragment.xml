<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">

	<Label
		id="PaidAmountId"
		labelFor="PaidAmount"
		text="{/#C_AbpPaymentType/PaidAmountInPaytCurrency/@sap:label}" />
	<ObjectNumber
		id="PaidAmount"
		number="{ path: 'PaidAmountInPaytCurrency', formatter: '.formatter.longFloatValue' }"
		unit="{PaymentCurrency}"/>

</core:FragmentDefinition>